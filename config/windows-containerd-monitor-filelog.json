{
	"plugin": "filelog",
	"pluginConfig": {
		"timestamp": "^time=\"(\\S*)\"",
		"message": "msg=\"([^\n]*)\"",
		"timestampFormat": "2006-01-02T15:04:05.999999999-07:00"
	},
	"logPath": "C:\\Program Files\\containerd\\containerd.log",
	"lookback": "5m",
	"bufferSize": 10,
	"source": "containerd",
	"conditions": [],
	"rules": [
		{
			"type": "temporary",
			"reason": "MissingPigz",
			"pattern": "unpigz not found.*"
		},
		{
			"type": "temporary",
			"reason": "IncompatibleContainer",
			"pattern": ".*CreateComputeSystem.*"
		}
	]
}
